{
  "name": "vaultrunner",
  "version": "0.3.1",
  "description": "MCP server for VaultRunner - 1Password credentials for Claude browser automation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "vaultrunner": "dist/cli/index.js",
    "vaultrunner-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp -r src/dashboard/public dist/dashboard/",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "prepublishOnly": "cp ../../README.md . && npm run build",
    "postinstall": "echo '' && echo '━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━' && echo '✓ VaultRunner installed!' && echo '' && echo 'Requirements:' && echo '  • Claude Max subscription (for Claude Code + Claude for Chrome)' && echo '  • 1Password Desktop App (for biometric unlock)' && echo '' && echo 'Next: Run \"vaultrunner setup\" for full setup instructions.' && echo '━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━' && echo ''"
  },
  "keywords": [
    "1password",
    "mcp",
    "claude",
    "claude-code",
    "claude-for-chrome",
    "credentials",
    "browser-automation"
  ],
  "author": "Daniel Mason",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anon-dot-com/vaultrunner.git",
    "directory": "packages/mcp-server"
  },
  "homepage": "https://github.com/anon-dot-com/vaultrunner#readme",
  "bugs": {
    "url": "https://github.com/anon-dot-com/vaultrunner/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^11.0.0",
    "commander": "^14.0.0",
    "execa": "^9.0.0",
    "express": "^4.21.0",
    "googleapis": "^144.0.0",
    "open": "^11.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
