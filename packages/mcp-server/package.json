{
  "name": "vaultrunner",
  "version": "0.1.0",
  "description": "MCP server for VaultRunner - secure 1Password autofill for Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "vaultrunner": "dist/cli/index.js",
    "vaultrunner-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp -r src/dashboard/public dist/dashboard/",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "1password",
    "mcp",
    "claude",
    "claude-code",
    "autofill",
    "security",
    "browser-automation",
    "credentials"
  ],
  "author": "Daniel Mason",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anon-dot-com/vaultrunner.git",
    "directory": "packages/mcp-server"
  },
  "homepage": "https://github.com/anon-dot-com/vaultrunner#readme",
  "bugs": {
    "url": "https://github.com/anon-dot-com/vaultrunner/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@puppeteer/browsers": "^2.11.0",
    "@types/express": "^5.0.6",
    "better-sqlite3": "^11.0.0",
    "commander": "^14.0.2",
    "execa": "^9.0.0",
    "express": "^5.2.1",
    "googleapis": "^144.0.0",
    "open": "^11.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
